<h1>Group </h1>

<label for="group_name"><%= link_to group.name, group_path(group) %></label>
<ul>
  <% group.users.each do |user| %>
    <li><%= link_to user.username, user_path(user) %></li>
  <% end %>
</ul>

<%= button_to "Update Members", group_update_members_path(group), method: :get %>
<%= button_to "Edit Group", edit_group_path(group), method: :get %>

