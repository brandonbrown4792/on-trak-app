<%= form_with model: item, method: :post do |f| %>
  <%= f.label :content %>
  <%= f.text_field :content %><br>

  <%= f.label :due_date %>
  <%= f.text_field :due_date %><br>

  <%= f.label :priority %>
  <%= f.select :priority, item_priorities %><br>

  <%= f.collection_select :assigned_user, item.list.users, :id, :username %>
<% end %>